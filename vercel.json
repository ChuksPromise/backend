{
    "version": 2,
    "builds": [
      { "src": "api/signin.js", "use": "@vercel/node" },
      { "src": "/api/emailverify.js", "use": "@vercel/node" },
      { "src": "/api/code.js", "use": "@vercel/node" },
      { "src": "public/index.html", "use": "@vercel/static" },
      { "src": "public/emailverify.html", "use": "@vercel/static" },
      { "src": "public/code.html", "use": "@vercel/static" }
      
    ],
    "routes": [
      { "src": "/api/signin", "dest": "/api/signin.js" },
      { "src": "/api/emailverify", "dest": "/api/emailverify.js" },
      { "src": "/api/code", "dest": "/api/code.js" },
      { "src": "/", "dest": "/public/index.html"},
      { "src": "/api/emailverify", "dest": "/public/emailverfify.html"},
      { "src": "/api/code", "dest": "/public/code.html"}
    ]
  }
  